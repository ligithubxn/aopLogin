<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aopLog.demo.ssodemo.mapper.UserMapper" >
  <resultMap id="BaseResultMap" type="com.aopLog.demo.ssodemo.dataobject.User" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="user_name" property="userName" jdbcType="VARCHAR" />
    <result column="password" property="password" jdbcType="VARCHAR" />
    <result column="lastOutTime" property="lastOutTime" jdbcType="TIMESTAMP" />
  </resultMap>
  <update id="updateByPrimaryKey" parameterType="com.aopLog.demo.ssodemo.dataobject.User" >
    update user set
    <if test="userName != null and userName != ''">
      user_name = #{userName,jdbcType=VARCHAR},
    </if>
    <if test="password != null and password != ''">
      password = #{password,jdbcType=VARCHAR},
    </if>
      lastOutTime = now()
    where id = #{id,jdbcType=INTEGER}
  </update>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from user
    where id = #{id,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.aopLog.demo.ssodemo.dataobject.User" keyProperty="id" useGeneratedKeys="true">
    insert into user (id, user_name, password, 
      lastOutTime)
    values (#{id,jdbcType=INTEGER}, #{userName,jdbcType=VARCHAR}, #{password,jdbcType=VARCHAR}, 
      now())
  </insert>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select id, user_name, password, lastOutTime
    from user
    where id = #{id,jdbcType=INTEGER}
  </select>
  <select id="selectAll" resultMap="BaseResultMap" >
    select id, user_name, password, lastOutTime
    from user
  </select>
</mapper>